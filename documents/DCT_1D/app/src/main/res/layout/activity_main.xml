<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="5dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:paddingTop="5dp"
    tools:context="com.example.buixu.dct_1d.MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kết quả"
        android:id="@+id/button_ketqua"
        android:layout_below="@+id/linearLayout2"
        android:layout_centerHorizontal="true"
        android:elevation="3dp" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:weightSum="0.5"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout"
        android:paddingLeft="5dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="f0="
            android:id="@+id/textView"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="f1="
            android:id="@+id/textView2"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="f2="
            android:id="@+id/textView3"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="f3="
            android:id="@+id/textView4"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="f4="
            android:id="@+id/textView5"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="f5="
            android:id="@+id/textView6"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="f6="
            android:id="@+id/textView7"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="f7="
            android:id="@+id/textView8"
            android:gravity="center" />

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="120dp"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/linearLayout"
        android:id="@+id/linearLayout2"
        android:layout_alignBottom="@+id/linearLayout">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_f0"
            android:gravity="center"
            android:hint="Enter f0" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_f1"
            android:gravity="center"
            android:hint="Enter f1" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_f2"
            android:gravity="center"
            android:hint="Enter f2" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_f3"
            android:gravity="center"
            android:hint="Enter f3" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_f4"
            android:gravity="center"
            android:hint="Enter f4" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_f5"
            android:gravity="center"
            android:hint="Enter f5" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_f6"
            android:gravity="center"
            android:hint="Enter f6" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_f7"
            android:gravity="center"
            android:hint="Enter f7" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/linearLayout2"
        android:layout_toEndOf="@+id/linearLayout2"
        android:id="@+id/linearLayout3"
        android:layout_alignBottom="@+id/linearLayout">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="F0="
            android:id="@+id/textView9"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="F1="
            android:id="@+id/textView10"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="F2="
            android:id="@+id/textView11"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="F3="
            android:id="@+id/textView12"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="F4="
            android:id="@+id/textView13"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="F5="
            android:id="@+id/textView14"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="F6="
            android:id="@+id/textView15"
            android:gravity="center" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="F7="
            android:id="@+id/textView16"
            android:gravity="center" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/linearLayout3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/linearLayout3"
        android:paddingRight="5dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_F0"
            android:gravity="center"
            android:hint="___________" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_F1"
            android:gravity="center"
            android:hint="___________" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_F2"
            android:gravity="center"
            android:hint="___________" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_F3"
            android:gravity="center"
            android:hint="___________" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_F4"
            android:gravity="center"
            android:hint="___________" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_F5"
            android:gravity="center"
            android:hint="___________" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_F6"
            android:gravity="center"
            android:hint="___________" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView_F7"
            android:gravity="center"
            android:hint="___________" />
    </LinearLayout>
</RelativeLayout>
